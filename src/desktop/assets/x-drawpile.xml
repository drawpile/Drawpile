<mime-info xmlns='http://www.freedesktop.org/standards/shared-mime-info'>
	<mime-type type="application/x-drawpile-recording">
		<comment>Drawpile recording</comment>
		<magic priority="50">
			<match type="string" value="DPRECR\000" offset="0" />
		</magic>
		<glob pattern="*.dprec"/>
	</mime-type>
</mime-info>
